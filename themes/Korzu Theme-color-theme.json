{
  "name": "Korzu Theme",
  "colors": {
    "list.inactiveSelectionBackground": "#043e59",
    "editor.background": "#132939",
    "tab.activeForeground": "#7cfa6f",
    "tab.inactiveForeground": "#98bfe8",
    "editor.foreground": "#afe4ff",
    "activityBar.background": "#1e303d",
    "activityBar.activeBorder": "#8719dd",
    "activityBar.activeBackground": "#243c4d",
    "editorHoverWidget.background": "#3c586c",
    "tab.inactiveBackground": "#09192c",
    "tab.activeBorderTop": "#6cff39",
    "tab.hoverBackground": "#063d76",
    // "terminal.background": "#174144",
    // "terminal.foreground": "#ff2082",
    "editorCursor.foreground": "#0aff33",
    "sideBar.background": "#0b293a",
    "sideBar.foreground": "#bfe2f5",
    "list.hoverBackground": "#07577e",
    "scrollbarSlider.background": "#535d62",
    "activityBarBadge.background": "#152e3e",
    "sideBarTitle.foreground": "#59a2d6"
  },

  "tokenColors": [
    {
      "name": "react-import",
      "scope": ["keyword.control.import.js.jsx"],
      "settings": {
        "foreground": "#a404ee",
        "fontStyle": "bold"
      }
    },
    {
      "name": "react-as",
      "scope": ["keyword.control.as.js.jsx"],
      "settings": {
        "foreground": "#dd20ff",
        "fontStyle": "bold"
      }
    },
    {
      "name": "react-abc",
      "scope": [
        "support.class.component.js.jsx",
        "entity.name.tag.js.jsx",
        "meta.tag.without-attributes.js.jsx",
        "meta.block.js.jsx",
        "meta.arrow.js.jsx",
        "meta.var.expr.js.jsx",
        "source.js.jsx"
      ],
      "settings": {
        "foreground": "#f90c6d",
        "fontStyle": "bold"
      }
    },
    {
      "name": "react-return",
      "scope": [
        "keyword.control.flow.js.jsx",
        "meta.block.js.jsx",
        "meta.arrow.js.jsx",
        "meta.var.expr.js.jsx",
        "source.js.jsx"
      ],
      "settings": {
        "foreground": "#a60cf9",
        "fontStyle": "bold"
      }
    },
    {
      "name": "react-conditionals",
      "scope": ["keyword.control.conditional.js.jsx"],
      "settings": {
        "foreground": "#f9f525",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#76d676"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#c0e6f9"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#ff0000"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#ff0000"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#ad5fff"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "keyword.control",
        "constant.other.color",
        "punctuation",
        "meta.tag",
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "foreground": "#22ebfc"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#ff009e"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#00b6ff"
      }
    },
    {
      "name": "readme-title",
      "scope": ["entity.name.section.markdown"],
      "settings": {
        "foreground": "#f92552",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Block Level Variables",
      "scope": ["meta.block variable.other.property"],
      "settings": {
        "foreground": "#dbfc0a"
      }
    },
    {
      "name": "Block Level Variables",
      "scope": ["meta.block variable.other"],
      "settings": {
        "foreground": "#93e0ff"
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": ["support.other.variable", "string.other.link"],
      "settings": {
        "foreground": "#f07178"
      }
    },
    {
      "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape",
        "variable.parameter",
        "keyword.other.unit",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#f077fe"
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "entity.other.inherited-class",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "foreground": "#17f989"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name",
        "support.type",
        "support.class",
        "support.other.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter",
        "support.type.sys-types"
      ],
      "settings": {
        "foreground": "#39faf1"
      }
    },
    {
      "name": "Entity Types",
      "scope": ["support.type"],
      "settings": {
        "foreground": "#B2CCD6"
      }
    },
    {
      "name": "CSS Class and Support",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": {
        "foreground": "#17f989"
      }
    },
    {
      "name": "Sub-methods",
      "scope": [
        "entity.name.module.js",
        "variable.import.parameter.js",
        "variable.other.class.js"
      ],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Language methods",
      "scope": ["variable.language"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ff6e5f"
      }
    },
    {
      "name": "entity.name.method.js",
      "scope": ["entity.name.method.js"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "meta.method.js",
      "scope": [
        "meta.class-method.js entity.name.function.js",
        "variable.function.constructor"
      ],
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#23b6fa"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "text.html.basic entity.other.attribute-name.html",
        "text.html.basic entity.other.attribute-name"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FFCB6B"
      }
    },
    {
      "name": "CSS Classes",
      "scope": ["entity.other.attribute-name.class"],
      "settings": {
        "foreground": "#24e4fe"
      }
    },
    {
      "name": "CSS ID's",
      "scope": ["source.sass keyword.control"],
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "Inserted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#ff0000"
      }
    },
    {
      "name": "Deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#ff0000"
      }
    },
    {
      "name": "Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "URL",
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "tag.decorator.js entity.name.tag.js",
        "tag.decorator.js punctuation.definition.tag.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "ES7 Bind Operator",
      "scope": [
        "source.js constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FF5370"
      }
    },
